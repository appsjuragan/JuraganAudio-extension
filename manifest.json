{
   "manifest_version": 3,
   "name": "JuraganAudio: Bass Boost, EQ Any Audio!",
   "version": "1.0.1",
   "author": "AppsJuragan inc.",
   "description": "EQ any audio you find on the web, live! Crank the bass, dim the highs, up the vocals: all with JuraganAudio!",
   "short_name": "JuraganAudio",
   "background": {
      "service_worker": "service-worker.js",
      "type": "module"
   },
   "action": {
      "default_icon": "assets/juraganaudio128.png",
      "default_popup": "popup.html"
   },
   "icons": {
      "128": "assets/juraganaudio128.png",
      "16": "assets/juraganaudio16.png",
      "32": "assets/juraganaudio32.png",
      "48": "assets/juraganaudio48.png",
      "64": "assets/juraganaudio64.png"
   },
   "permissions": [
      "activeTab",
      "tabCapture",
      "tabs",
      "storage",
      "offscreen"
   ],
   "web_accessible_resources": [
      {
         "resources": [
            "worklet/audio-processor.js",
            "worklet/juragan_audio_dsp.js",
            "worklet/juragan_audio_dsp_bg.wasm",
            "worklet/polyfill.js",
            "worklet/modules/filters.js",
            "worklet/modules/fft.js",
            "worklet/modules/dynamics.js",
            "worklet/modules/sbr.js"
         ],
         "matches": [
            "<all_urls>"
         ]
      }
   ],
   "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
   },
   "minimum_chrome_version": "109",
   "update_url": "https://clients2.google.com/service/update2/crx"
}